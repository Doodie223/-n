    }

    .sidebar {
        width: 25%;
        padding: 20px;
        background-color: #f4f4f4;
        border-right: 1px solid #ddd;
        margin-top: 65px;
    }

    .content {
        width: 75%;
        padding: 20px;
    }

    h2 {
        margin-top: 0;

    }

    .font-color {
        color: white;
    }

    .filter-group {
        margin-bottom: 20px;
    }

    .filter-group label {
        display: block;
        margin-bottom: 5px;
        font-weight: bold;
        color: #333;
    }

    .filter-group select,
    .filter-group input[type="range"] {
        width: 100%;
        padding: 10px;
        margin-bottom: 10px;
        border: 1px solid #ccc;
        border-radius: 5px;
        box-sizing: border-box;
        font-size: 1em;
        background-color: #fff;
        transition: border-color 0.3s;
    }

    .filter-group select:focus,
    .filter-group input[type="range"]:focus {
