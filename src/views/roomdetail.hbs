        border: 1px solid #ddd;
        background-color: #fff;
        transition: transform 0.3s, box-shadow 0.3s;
    }

    .room:hover {
        transform: scale(1.05);
        box-shadow: 0 10px 20px rgba(0, 0, 0, 0.1);
    }

    .room img {
        width: 400px;
        height: 266px;
        object-fit: cover;
        margin-right: 10px;
    }

    .room-details {
        display: flex;
        flex-direction: column;
        justify-content: space-between;
        flex-grow: 1;
    }

    .room-name {
        font-size: 1.2em;
        font-weight: bold;
    }

    .room-price {
        color: #007BFF;
        font-size: 1em;
    }
